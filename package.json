{
  "name": "magic-scroll",
  "version": "1.0.0",
  "description": "It is a notes app",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch .env --watch src/**/*.ts src/index.ts --watch prisma/schema.prisma",
    "start": "node dist/index.js",
    "build": "prisma generate && tsc",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:generate@serverless": "prisma generate --no-engine",
    "prisma:push": "prisma db push",
    "prisma:flush": "ts-node ./src/seeder/dbFlush.ts",
    "prisma:user-seeder": "ts-node ./src/seeder/user.ts"
  },
  "author": "Rizqiansyah Ramadhan",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "@prisma/extension-accelerate": "^1.1.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "firebase-admin": "^12.3.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@prisma/adapter-pg": "^5.18.0",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.1.0",
    "@types/pg": "^8.11.6",
    "nodemon": "^3.1.4",
    "prisma": "^5.18.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  }
}
